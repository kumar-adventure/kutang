<%= simple_form_for [:admin, @make] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %><br>
    <%= f.association :category, prompt: 'Select Category' %>
  </div>

  <div class="form-actions" style='text-align: left'>
    <%= f.submit %>
  </div>
<% end %>
