<%= simple_form_for [:admin, @model] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %><br>
    <%= f.association :make, prompt: 'Select Make' %>
  </div>

  <div class="form-actions" style="text-align: left">
    <%= f.button :submit %>
  </div>
<% end %>
